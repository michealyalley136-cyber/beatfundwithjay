{
  "expo": {
    "name": "BeatFund",
    "slug": "beatfund",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "beatfund",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#020617"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beatfund.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#020617"
      },
      "package": "com.beatfund.app"
    },
    "web": {
      "bundler": "metro"
    }
  }
}

